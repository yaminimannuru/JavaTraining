package com.training.basic;

public class Datatypesx {
	 public static void main(String[] args) {
		 //predefined data type(primitive datatype)
		int a = 10;
		char c = 'a';
		boolean b= true;
		float f = 35.5f;
		double d = 355553.0;
			
		//non primitive datatype
		String s = "Hello h my age 24";
		//Array 
		int a1[]= {1,2,3,4,5};
	} 
	}
