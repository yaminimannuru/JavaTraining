package com.practise;

import java.util.Scanner;

public class Switchex {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int dayOfWeek = s.nextInt();

		String dayName;

		switch (dayOfWeek) {
		case 1:
			dayName = "Sunday";
			break;
		case 2:
			dayName = "Monday";
			break;
		case 3:
			dayName = "Tuesday";
			break;
		case 4:
			dayName = "Wednesday";
			break;
		case 5:
			dayName = "Thursday";
			break;
		case 6:
			dayName = "Friday";
			break;
		case 7:
			dayName = "Saturday";
			break;
		default:
			dayName = "Invalid day";
			break;
		}
	}
}
