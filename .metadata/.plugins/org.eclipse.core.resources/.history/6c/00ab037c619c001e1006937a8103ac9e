package com.practise;

import java.util.Scanner;

public class Printnum {
	public static void main(String[] args) {

		/*
		 * Scanner sc = new Scanner(System.in); System.out.println("age"); int i =
		 * sc.nextInt(); System.out.println("name"); String s = sc.next();
		 * System.out.println("fathername"); String st = sc.next();
		 * System.out.println("age: " + i); System.out.println("name: " + s);
		 * System.out.println("fathername: " + st);
		 */
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the value of n");
		int n = sc.nextInt();
		//10
		for(int i=1;i<=n;i++){
			//for(initialisation;condition;increment\decrement)
			System.out.println(i);
		}
		
	}

}


